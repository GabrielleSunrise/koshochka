{
  "name": "Запускаем кошочку",
  "description": "Кошочка ходит внизу сайта туды-сюды",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "koshochka.html"
  },
  "content_scripts": [
     {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
     }
  ]
}
