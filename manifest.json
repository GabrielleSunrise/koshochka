{
  "name": "Запускаем кошочку",
  "description": "Кошочка ходит внизу сайта туды-сюды",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "img/icon-16.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
    },
    "default_popup": "koshochka.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
     {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
     }
  ]
}
